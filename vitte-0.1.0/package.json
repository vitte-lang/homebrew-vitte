{
  "name": "vitte-workspace",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "cargo build --workspace --all-features",
    "release": "cargo build --workspace --release",
    "check": "cargo check --workspace --all-features",
    "fmt": "cargo fmt --all",
    "fmt:check": "cargo fmt --all -- --check",
    "clippy": "cargo clippy --workspace --all-features -- -D warnings",
    "lint": "npm run fmt:check && npm run clippy",
    "test": "cargo test --workspace --all-features",
    "bench": "cargo bench --workspace",
    "doc": "cargo doc --workspace --all-features --no-deps",
    "clean": "cargo clean",
    "wasm:build": "wasm-pack build crates/vitte-wasm --target web",
    "wasm:pack": "wasm-pack pack crates/vitte-wasm",
    "wasm:test": "wasm-pack test --node crates/vitte-wasm",
    "tree": "cargo tree -e features"
  },
  "devDependencies": {}
}
